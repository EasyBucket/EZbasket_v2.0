{
  "manifest_version": 2,

  "name": "ez_basket_chromeExtension",
  "description": "easily add favorite product and compare price",
  "version": "2.1.0",

  "browser_action": {
    "default_icon": "img/cart.png",
    "default_popup": "main.html"
  },

  "permissions": [
    "<all_urls>",
    "storage",
    "activeTab"
  ]
}
